{
  "name": "react-webpack-rails-tutorial",
  "version": "1.1.1",
  "description": "Code from the React Webpack tutorial.",
  "main": "server-express.js",
  "engines": {
    "node": "5.0.0",
    "npm": "3.3.6"
  },
  "scripts": {
    "frontend:postinstall": "cd clients/frontend && npm install",
    "frontend:test": "rspec && npm run test:client && npm run lint",
    "frontend:test:client": "(cd clients/frontend && npm run test --silent)",
    "frontend:lint": "(cd clients/frontend && npm run lint --silent)",
    "frontend:build:clean": "rm app/assets/bl/frontend/webpack/* && rm app/assets/bl/backend/webpack/*",
    "frontend:build:production:client": "(cd clients/frontend && npm run build:production:client --silent)",
    "frontend:build:production:server": "(cd clients/frontend && npm run build:production:server --silent)",
    "frontend:build:client": "(cd clients/frontend && npm run build:client --silent",
    "frontend:build:server": "(cd clients/frontend && npm run build:server --silent)",
    "frontend:build:dev:client": "(cd clients/frontend && npm run build:dev:client --silent)",
    "frontend:build:dev:server": "(cd clients/frontend && npm run build:dev:server --silent)",
    "frontend:hot-assets": "(cd clients/frontend && npm run hot-assets)",
    "frontend:start": "(cd clients/frontend && npm run start --silent)",
    "backend:postinstall": "cd clients/backend && npm install",
    "backend:test": "rspec && npm run test:client && npm run lint",
    "backend:test:client": "(cd clients/backend && npm run test --silent)",
    "backend:lint": "(cd clients/backend && npm run lint --silent)",
    "backend:build:production:client": "(cd clients/backend && npm run build:production:client --silent)",
    "backend:build:production:server": "(cd clients/backend && npm run build:production:server --silent)",
    "backend:build:client": "(cd clients/backend && npm run build:client --silent",
    "backend:build:server": "(cd clients/backend && npm run build:server --silent)",
    "backend:build:dev:client": "(cd clients/backend && npm run build:dev:client --silent)",
    "backend:build:dev:server": "(cd clients/backend && npm run build:dev:server --silent)",
    "backend:hot-assets": "(cd clients/backend && npm run hot-assets)",
    "backend:start": "(cd clients/backend && npm run start --silent)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shakacode/react-webpack-rails-tutorial.git"
  },
  "keywords": [
    "react",
    "tutorial",
    "comment",
    "example"
  ],
  "author": "justin808",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shakacode/react-webpack-rails-tutorial/issues"
  },
  "homepage": "https://github.com/shakacode/react-webpack-rails-tutorial",
  "dependencies": {},
  "devDependencies": {}
}
